{
  "name": "baidu-pan-downloader",
  "version": "2.3.0",
  "description": "A download manager for Baidu Yun",
  "main": "dist/index.js",
  "keywords": [
    "baidu",
    "baiduyun",
    "pandownload"
  ],
  "author": "Dotennin",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch",
    "clean": "rimraf build/*",
    "build": "npm run clean && react-app-rewired build && cp ./build/static/js/main.js ./dist/bundle.user.js",
    "start": "react-app-rewired start",
    "eslint": "eslint --fix src/*.*",
    "release": "standard-version",
    "eject": "react-app-rewired eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add dist/ && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@types/node": "^12.12.49",
    "@types/react": "^16.9.42",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.1.1",
    "typesafe-actions": "^4.4.2",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-plugin-styled-components": "^1.10.7",
    "customize-cra": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "standard-version": "^8.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
