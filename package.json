{
  "name": "baidu-pan-downloader",
  "version": "1.4.2",
  "description": "A download manager for Baidu Yun",
  "main": "dist/index.js",
  "keywords": [
    "baidu",
    "baiduyun",
    "pandownload"
  ],
  "author": "Dotennin",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "webpack:dev": "webpack --mode development",
    "webpack:prod": "webpack --mode production",
    "watch": "webpack --watch",
    "clean": "rimraf dist/*",
    "clean:public": "rimraf public/*",
    "build:dev": "npm-run-all clean:public webpack:dev",
    "build": "npm-run-all clean:public webpack:prod",
    "start": "webpack-dev-server",
    "eslint": "eslint --fix src/*.*",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add public/ && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "standard-version": "^8.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
